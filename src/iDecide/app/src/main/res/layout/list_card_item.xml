<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/linear_list_card_item"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:stackHeaderHeight="160dp"
    android:animateLayoutChanges="true">

    <FrameLayout
        android:id="@+id/frame_list_card_item"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/shape_rectangle_with_radius">

        <TextView
            android:id="@+id/text_list_card_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:textColor="@android:color/white"
            android:textSize="40sp"
            android:textStyle="bold"
            tools:text="12"/>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_list_content"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation = "vertical"
        >

        <ImageView
            android:contentDescription="Computer Society"
            android:id="@+id/home_image1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:src="@drawable/laptop1" />
        <ImageView
            android:contentDescription="Computer Society"
            android:id="@+id/home_image2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:src="@drawable/laptop1" />
        <ImageView
            android:contentDescription="Computer Society"
            android:id="@+id/home_image3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:src="@drawable/laptop1" />
        <ImageView
            android:contentDescription="Computer Society"
            android:id="@+id/home_image4"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:src="@drawable/laptop1" />
        <ImageView
            android:contentDescription="Computer Society"
            android:id="@+id/home_image5"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:src="@drawable/laptop1" />
        </FrameLayout>
</LinearLayout>